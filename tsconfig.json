{
    "compilerOptions": {
      "target": "es2018",
      "module": "commonjs",
      "sourceMap": true,
      "outDir": "./dist",
      "strict": true,
      "noImplicitAny": false,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true
    }
  }
  